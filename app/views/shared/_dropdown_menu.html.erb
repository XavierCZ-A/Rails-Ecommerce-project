<!-- Main modal -->
<div id="timeline-modal" tabindex="-1" aria-hidden="true" class="">
  <div>
    <!-- Modal content -->
    <div>
      <!-- Modal body -->
      <div class="p-5">
        <ol class="relative border-s-2 border-indigo-500 dark:border-indigo-600 ms-3.5 h-32 mb-3">
          <li class="mb-10 ms-5">
            <span class="absolute flex items-center justify-center w-3 h-3 bg-gray-100 rounded-full -start-1.5 ring-2 ring-white  dark:bg-indigo-600">
              <%#= icon('fa-solid', 'warehouse', class: 'bg-white') %>
            </span>
            <%= link_to products_path, class: 'flex items-start text-sm font-semibold text-gray-700', role: 'menuitem', tabindex: '-1', id: 'menu-item-0' do %>
              <span>Inventory</span>
            <% end %>
          </li>
          <li class="mb-10 ms-5">
            <span class="absolute flex items-center justify-center w-3 h-3 bg-gray-100 rounded-full -start-1.5 ring-2 ring-white  dark:bg-indigo-600">
              <%#= icon('fa-solid', 'circle-plus', class: 'bg-white') %>
            </span>
            <%= link_to new_product_path, class: 'flex items-start text-sm font-semibold text-gray-700', role: 'menuitem', tabindex: '-1', id: 'menu-item-0' do %>
              <span>Create product</span>
            <% end %>
          </li>
          <li class="ms-5">
            <span class="absolute flex items-center justify-center w-3 h-3 bg-gray-100 rounded-full -start-1.5 ring-2 ring-white  dark:bg-indigo-600">
              <%#= icon('fa-solid', 'cubes', class: 'bg-white') %>
            </span>
            <%= link_to stocks_path, class: 'flex justify-start text-sm font-semibold text-gray-700', role: 'menuitem', tabindex: '-1', id: 'menu-item-0' do %>
              <span>Stocks</span>
            <% end %>
          </li>
        </ol>
      </div>
    </div>
  </div>
</div>

